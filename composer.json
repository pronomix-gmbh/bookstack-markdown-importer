{
    "name": "pronomix-gmbh/bookstack-markdown-importer",
    "description": "BookStack extension to import Markdown files into books.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "league/commonmark": "^2.5",
        "ezyang/htmlpurifier": "^4.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "BookStackMarkdownImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookStackMarkdownImporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BookStackMarkdownImporter\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
